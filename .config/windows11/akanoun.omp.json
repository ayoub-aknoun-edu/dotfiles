{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "╭─[<#ffffff> {{ .UserName }} <#666666>from</>  {{ .HostName }}]</>",
          "foreground": "#7eb8da",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "root_icon": ""
          },
          "template": " {{ .Icon }}",
          "type": "root",
          "style": "plain",
          "foreground": "#ff6b6b"
        },
        {
          "template": "[<#ffffff></> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB]",
          "foreground": "#be9ddf",
          "type": "sysinfo",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .PhysicalPercentUsed 80.0 }}#ff6b6b{{ else if gt .PhysicalPercentUsed 60.0 }}#ffd93d{{ else }}#be9ddf{{ end }}"
          ]
        },
        {
          "properties": {
            "style": "dallas",
            "threshold": 500
          },
          "template": "[<#ffffff></> {{ .FormattedMs }}s]",
          "foreground": "#be9ddf",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": "[{{ if eq .Code 0 }}<#6bcf7f> {{ .Code }}</>{{ else }}<#ff6b6b> {{ .Code }}</>{{ end }}]",
          "foreground": "#ffa5d8",
          "type": "status",
          "style": "plain"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#98bfad",
          "properties": {
            "time_format": "15:04:05"
          },
          "template": "[<#ffffff></> {{ .CurrentDate | date .Format }}]"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "full",
            "folder_separator_icon": " 󰅂 "
          },
          "template": "│ <#98bfad> {{ .Path }}</>",
          "foreground": "#7eb8da",
          "type": "path",
          "style": "plain"
        },
        {
          "template": " <#4584b6> </>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}<#ffd93d>({{ .Venv }})</> {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#4584b6",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }} +{{ add .Working.Added .Working.Untracked }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} | ⧆+{{ add .Staging.Added .Staging.Untracked }} ~{{ .Staging.Modified }} -{{ .Staging.Deleted }}  {{ .StashCount }} ",
          "foreground": "#ffd93d",
          "type": "git",
          "style": "plain"
        },
        {
          "template": "  {{ .Profile }}{{if .Region}} @ {{ .Region }}{{ end }}",
          "foreground": "#ff9900",
          "type": "aws",
          "style": "plain"
        },
        {
          "template": " ☸ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "foreground": "#326ce5",
          "type": "kubectl",
          "style": "plain"
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "#00acd7",
          "properties": {
            "display_mode": "context"
          },
          "template": "{{ if ne .Context \"desktop-linux\" }}   {{ .Context }}{{ end }}"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if lt .Percentage 20}}#ff6b6b{{else if lt .Percentage 40}}#ffd93d{{else}}#6bcf7f{{end}}"
          ],
          "properties": {
            "charged_icon": "  ",
            "charging_icon": "  ",
            "discharging_icon": "  "
          },
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }}"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "template": "╰─❯",
          "foreground": "#7eb8da",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": "{{ if gt .Code 0 }}{{ else }}{{ end }} ",
          "foreground": "#6bcf7f",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff6b6b{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}"
}
